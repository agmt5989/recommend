<!--/ Section Blog-Single Star /-->
<section class="blog-wrapper sect-pt4" id="blog">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="widget-sidebar sidebar-search">
                    <h5 class="sidebar-title">Search</h5>
                    <div class="sidebar-content">
                        <form id="finder" method="get" class="form-inline">
                            <div class="input-group">
                              <label class="sr-only" for="findid">ID</label>
                                <input type="text" class="form-control mb-3 mr-sm-3" placeholder="Enter id..." aria-label="Enter id..." id="findid">
                              <label class="sr-only" for="findno">Number</label>
                                <input type="text" class="form-control mb-3 mr-sm-3" placeholder="Enter number..." aria-label="Enter number..." id="findno">
                              <button type="submit" class="btn btn-primary mb-3">Submit</button>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="widget-sidebar">
                    <h5 class="sidebar-title">Results</h5>
                    <div class="sidebar-content">
                      <div class="box">
                        <div class="box-header">
                          <h3 class="box-title">All Senate Members</h3>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                          <table id="users" class="table table-bordered table-hover">
                            <thead>
                            <tr>
                              <th class="text-center">S/N</th>
                              <th class="text-center">Code</th>
                              <th class="text-center">Description</th>
                              {{!--<th>Email</th>--}}
                              {{!--<th>Phone</th>--}}
                            </tr>
                            </thead>
                            <tbody>
                            {{# each everyone}}
                              <tr class='click-row' data-href='/admin/user/{{_id}}'>
                                <td>{{inc @index}}</td>
                                <td>{{sentence designation}}</td>
                                <td>{{fullname}}</td>
                                {{!--<td>{{initials}}</td>--}}
                                {{!--<td>{{{replace email ',' '<br>'}}}</td>--}}
                                {{!--<td>{{{replace phone ',' '<br>'}}}</td>--}}
                              </tr>
                            {{/each}}
                            </tbody>
                          </table>
                        </div>
                        <!-- /.box-body -->
                      </div>
                      <!-- /.box -->
                    </div>
                </div>
                <div class="widget-sidebar">
                    <h5 class="sidebar-title">Archives</h5>
                    <div class="sidebar-content">
