<!--/ Section Blog-Single Star /-->
<section class="blog-wrapper sect-pt4" id="blog">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="widget-sidebar sidebar-search">
          <h5 class="sidebar-title">Select Customer ID</h5>
          <div class="sidebar-content">
            <table id="no-data" class="table table-hover"></table>
            <form id="finder" method="get" class="form-inline">
              <div class="input-group">
                <label class="sr-only" for="findid">ID</label>
                <input type="text" class="form-control mb-3 mr-sm-3" placeholder="Enter id..." aria-label="Enter id..."
                       id="findid">
                <label class="sr-only" for="findno">Number</label>
                <input type="text" class="form-control mb-3 mr-sm-3" placeholder="Enter number..."
                       aria-label="Enter number..." id="findno">
                <button type="submit" class="btn btn-primary mb-3">Submit</button>
              </div>
            </form>
          </div>
        </div>
        <div class="widget-sidebar">
          <h5 class="sidebar-title">Results</h5>
          <div class="sidebar-content invisible" id="pbody">
            <div class="box">
              <div class="box-header">
                <h3 class="box-title">History</h3>
              </div>
              <!-- /.box-header -->
              <div class="box-body">
                <table class="table table-bordered table-hover" id="his">
                  <thead>
                  <tr>
                    <th class="text-center">S/N</th>
                    <th class="text-center">Product ID</th>
                    <th class="text-center">Description</th>
                    {{!--<th>Email</th>--}}
                    {{!--<th>Phone</th>--}}
                  </tr>
                  </thead>
                  <tbody id="real-body">
                  </tbody>
                </table>
              </div>
              <!-- /.box-body -->
            </div>
            <!-- /.box -->

            <div class="box">
              <div class="box-header">
                <h3 class="box-title">Recommendations</h3>
              </div>
              <!-- /.box-header -->
              <div class="box-body">
                <table class="table table-bordered table-hover" id="rec">
                  <thead>
                  <tr>
                    <th class="text-center">S/N</th>
                    <th class="text-center">Product ID</th>
                    <th class="text-center">Score</th>
                    <th class="text-center">Rank</th>
                    <th class="text-center">Description</th>
                    {{!--<th>Email</th>--}}
                    {{!--<th>Phone</th>--}}
                  </tr>
                  </thead>
                  <tbody id="reco-body">
                  </tbody>
                </table>
              </div>
              <!-- /.box-body -->
            </div>
            <!-- /.box -->
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
